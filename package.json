{
    "name": "flub",
    "author": "Joe Maidman",
    "license": "MIT",
    "version": "1.0.3",
    "description": "A javascript unit testing framework",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "flub": "./dist/app.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "format": "prettier --write '**/*.ts'",
        "build": "tsc -p tsconfig.build.json",
        "build:watch": "tsc --watch -p tsconfig.build.json",
        "bundle": "webpack --config ./webpack.config.js",
        "bundle:watch": "webpack --watch --config ./webpack.config.js",
        "release": "npm run build && npm publish",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@types/chokidar": "^1.7.2",
        "@types/clear-require": "^2.0.0",
