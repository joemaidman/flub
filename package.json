        "@types/glob": "^5.0.32",
        "@types/jsdom": "^11.0.2",
        "@types/lodash": "^4.14.74",
        "@types/node": "^13.11.1",
        "@types/ramda": "^0.24.13",
        "@types/require-from-string": "^1.2.0",
        "@types/stack-trace": "0.0.28",
        "better-stack-traces": "^1.1.0",
        "chokidar": "^1.7.0",
        "clean-stacktrace": "^1.1.0",
        "clear-require": "^2.0.0",
        "colors": "^1.1.2",
        "esprima": "^4.0.0",
        "estraverse": "^4.2.0",
        "find": "^0.2.7",
        "find-requires": "^0.2.4",
        "flags": "^0.1.3",
        "glob": "^7.1.2",
        "jsdom": "^11.2.0",
        "karma-es6-shim": "^1.0.0",
        "lodash": "^4.17.4",
        "manakin": "^0.5.1",
        "measure-time": "^3.1.0",
        "path": "^0.12.7",
        "pretty-error": "^2.1.1",
        "ramda": "^0.24.1",
        "require": "^2.4.20",
        "require-from-string": "^2.0.1",
        "stack-trace": "0.0.10",
        "tslib": "^1.7.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "@types/sinon": "^9.0.0",
        "awesome-typescript-loader": "^3.2.3",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
